{
  "name": "bootstrap-components",
  "version": "1.4.0",
  "main": "dist/index.js",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-dom": "^16.13.1"
  },
  "scripts": {
